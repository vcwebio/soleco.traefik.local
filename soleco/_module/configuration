#!/usr/bin/env bash

##### START MOULE CONFIGURATION SETUP #####
DOLLAR='$'
export MODECO_HOSTNAME="${DOLLAR}{CONTECO_HOSTNAME}"
export MODECO_MODULEPREFIX="${CONTECO_TYPE}_${CONTECO_NAME}"
export MODECO_URLPREFIX="${CONTECO_TYPE}"

export MODECO_CONFIGURATION_KEYS='\
$MODECO_HOSTNAME,\
$MODECO_MODULEPREFIX,\
$MODECO_URLPREFIX,
$MODECO_TRAEFIK_TRAEFIK_GATEWAY_PORT,
$MODECO_TRAEFIK_TRAEFIK_DASHBOARD_PORT'
export MODECO_CONFIGURATION_FILES="\
metrics_monitor_heartbeat_monitors_volume/traefik.yml"
##### END MOULE CONFIGURATION SETUP #####

##### START MOULE CONFIGURATION KEYVALUES #####
export MODECO_TRAEFIK_TRAEFIK_GATEWAY_PORT="80"
export MODECO_TRAEFIK_TRAEFIK_DASHBOARD_PORT="8080"
##### END MOULE CONFIGURATION KEYVALUES #####
